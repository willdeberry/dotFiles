[user]
    name = Will DeBerry
    email = willdeberry@gmail.com
[color]
    ui = auto
[merge]
    tool = emerge
[core]
    editor = vim
    autocrlf = input
    ignorecase = false
    pager = less -F -X
[alias]
    lg = log --oneline --graph --decorate
    ca = commit -a
    cl = clean -i
    cm = commit
    cp = cherry-pick
    co = checkout
    st = status
    mt = mergetool
[push]
    default = simple
[branch]
    autosetuprebase = always
